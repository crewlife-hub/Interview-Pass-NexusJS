# Google OAuth
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here
GOOGLE_CALENDAR_ID=your_google_calendar_id_here

# Google Calendar API (for OAuth scopes: calendar, gmail)
NEXT_PUBLIC_GOOGLE_CALENDAR_API_KEY=your_calendar_api_key_here

# Apps Script (if bridging to legacy)
APPS_SCRIPT_DEPLOYMENT_URL=your_apps_script_deployment_url_here
APPS_SCRIPT_SHARED_SECRET=your_apps_script_shared_secret_here

# Smartsheet API (future integration)
SMARTSHEET_API_TOKEN=your_smartsheet_api_token_here

# App URLs
NEXT_PUBLIC_API_BASE_URL=http://localhost:3001
NEXT_PUBLIC_WEB_BASE_URL=http://localhost:3000

# Environment
NODE_ENV=development
LOG_LEVEL=debug
